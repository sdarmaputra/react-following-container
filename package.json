{
  "name": "react-following-container",
  "version": "1.0.2",
  "description": "React JS following container",
  "main": "index.js",
  "author": "Surya Darma Putra <surya.igede@gmail.com> (https://github.com/sdarmaputra)",
  "repository": {
    "type": "git",
    "url": "https://github.com/sdarmaputra/react-following-container"
  },
  "bugs": {
    "url": "https://github.com/sdarmaputra/react-following-container/issues"
  },
  "keywords": [
    "react",
    "hoc",
    "following",
    "container"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "webpack-dev-server --hot --open",
    "build": "npm run build:js && npm run build:css",
    "build:js": "babel src/libs --out-dir dist/libs",
    "build:css": "node-sass src/libs -r -o dist/libs",
    "build:examples": "webpack -p",
    "test": "rm -rf coverage && karma start --single-run --browsers ChromeHeadless karma.config.js",
    "watch-test": "rm -rf coverage && karma start karma.config.js",
    "coveralls": "cat ./coverage/headlesschrome/lcov.info | node_modules/coveralls/bin/coveralls.js"
  },
  "dependencies": {
    "react": "^16.0.0"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "babelify": "^7.3.0",
    "browserify": "^14.4.0",
    "browserify-istanbul": "^3.0.1",
    "chai": "^4.1.2",
    "chai-enzyme": "beta",
    "coveralls": "^3.0.0",
    "css-loader": "^0.28.7",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "extract-text-webpack-plugin": "^3.0.1",
    "html-webpack-plugin": "^2.30.1",
    "jsdom": "^11.3.0",
    "jsdom-browser": "^0.5.1",
    "jsdom-global": "^3.0.2",
    "karma": "^1.7.1",
    "karma-browserify": "^5.1.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-scss-preprocessor": "^3.0.0",
    "mocha": "^4.0.1",
    "node-sass": "^4.5.3",
    "react-dom": "^16.0.0",
    "react-router-dom": "^4.2.2",
    "react-test-renderer": "^16.0.0",
    "sass-loader": "^6.0.6",
    "sassify": "^3.0.0",
    "sinon": "^4.0.1",
    "style-loader": "^0.19.0",
    "watchify": "^3.9.0",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1"
  }
}
